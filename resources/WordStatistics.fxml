<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="97.0" prefWidth="435.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.gui.WordStatistics">
   <children>
      <VBox prefHeight="101.0" prefWidth="435.0">
         <children>
            <Label fx:id="lblStatisticsFor" alignment="CENTER" prefHeight="17.0" prefWidth="602.0" text="Statistics for: &lt;word&gt;" textAlignment="CENTER">
               <font>
                  <Font name="Arial Black" size="32.0" />
               </font>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin>
            </Label>
            <HBox prefHeight="26.0" prefWidth="590.0">
               <children>
                  <Label text="Numbers of letters:">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblNumbersOfLetters" text="Label">
                     <font>
                        <Font name="Arial" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="5.0" top="3.0" />
                     </HBox.margin>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets left="10.0" />
               </VBox.margin></HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label text="Begins with a vowel:">
                     <font>
                        <Font name="Arial Black" size="14.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="lblBeginsWithVowel" text="Label">
                     <HBox.margin>
                        <Insets left="5.0" top="3.0" />
                     </HBox.margin>
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
